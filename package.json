    "scripts": {
        "preinstall": "cd libs/src/gulp-jsdoc3 && npm install && npm run build",
        "postinstall": "",
        "start": "npm run build && npx gulp compile-view && npx gulp watch",
        "snyk-protect": "npx snyk protect",
        "prepublish": "npm run snyk-protect",
        "check": "depcheck ./",
        "test": "echo \"Error: no test specified\" && exit 1",
        "component-build": "tsc -p tsconfig.formsaver.json",
        "build": "npx tsc -p tsconfig.build.json && npx tsc -p tsconfig.precompiler.json && npx tsc -p tsconfig.compiler.json",
        "ngrok": "node ./ngrok.js",
        "doc": "npm run doc:del && npm run doc:build && npm run doc:del",
        "doc:del": "rm -rf ./docs-src/php && rm -rf ./docs-src/.vuepress/public/js",
        "doc:dev": "npm run doc:build && node ./node_modules/vuepress/cli.js dev docs-src",
        "doc:build": "composer run-script doc && npm run build && gulp doc && node ./node_modules/vuepress/cli.js build docs-src"
    },
